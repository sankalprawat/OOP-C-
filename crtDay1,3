#include<bits/stdc++.h>
using namespace std ;

int maxProductTriplet(vector<int> nums) {
    sort(nums.begin(), nums.end());
    int n = nums.size() ;
    int prod = nums[n-1] * nums[n-2] * nums[n-3] ;
    return prod ;
}

int main () {
    vector<int> nums = {1,2,3,10,6} ;
    cout << maxProductTriplet(nums) << endl ;
    return 0 ;
}